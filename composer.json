{
  "name": "capusta/php-sdk",
  "description": "Capusta PHP SDK",
  "version": "1.0.6",
  "keywords": [
    "payment",
    "payments",
    "pay",
    "cards",
    "money",
    "sdk",
    "capusta.space",
    "capusta"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Capusta.Space Developers",
      "email": "support@capusta.space"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/log": "*",
    "guzzlehttp/psr7": "^1.4",
    "ext-apache": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "guzzlehttp/guzzle": "^6.5"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Guzzle Api Transport"
  },
  "autoload": {
    "psr-4": {
      "Capusta\\SDK\\": "src/"
    }
  }
}
