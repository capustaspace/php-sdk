{
  "name": "capusta/php-sdk",
  "description": "Capusta PHP SDK",
  "keywords": [
    "payment",
    "payments",
    "pay",
    "cards",
    "money",
    "sdk",
    "capusta.space",
    "capusta"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Capusta.Space Developers",
      "email": "support@capusta.space"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/log": ">=1.1",
    "guzzlehttp/psr7": "^1.6",
    "guzzlehttp/guzzle": "~7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.10"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Guzzle Api Transport"
  },
  "autoload": {
    "psr-4": {
      "Capusta\\SDK\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2-dev"
    }
  }
}
